<mujoco model="biped">
<compiler angle="radian" coordinate="local" inertiafromgeom="true"/>
<option gravity="0 0 -9.81" integrator="RK4" timestep="0.001"/>
<default>
<geom friction="10 3 1" condim="4" solref="0.002 1" solimp="0.9 0.95 0.001" contype="1" conaffinity="1"/>
<joint damping="2"/>
</default>
<worldbody>
<!--  Ground  -->
<geom name="ground" type="plane" size="8 8 0.1" pos="0 0 0"/>
<!--  SOLID stairs: rise=0.10, run=0.20  -->
<geom name="stair1" type="box" size="0.10 0.6 0.05" pos="0.30 0 0.05"/>
<geom name="stair2" type="box" size="0.20 0.6 0.10" pos="0.50 0 0.15"/>
<geom name="stair3" type="box" size="0.30 0.6 0.15" pos="0.70 0 0.25"/>
<geom name="stair4" type="box" size="0.40 0.6 0.20" pos="0.90 0 0.35"/>
<geom name="stair5" type="box" size="0.50 0.6 0.25" pos="1.10 0 0.45"/>
<!--  Torso  -->
<body name="torso" pos="0 0 1.0">
<geom name="torso_geom" type="box" size="0.15 0.15 0.25" mass="6"/>
<!--  Floating base  -->
<joint name="rootx" type="slide" axis="1 0 0" damping="10"/>
<joint name="rootz" type="slide" axis="0 0 1" damping="10"/>
<joint name="rootyaw" type="hinge" axis="0 0 1" range="0 0"/>
<joint name="theta" type="hinge" axis="0 -1 0" range="-0.4 0.4"/>
<!--  LEFT LEG  -->
<body pos="0 -0.1 -0.2">
<joint name="q1" type="hinge" axis="0 1 0" range="-1.2 0.6"/>
<!--  thigh visual only  -->
<geom name="left_thigh_geom" type="cylinder" fromto="0 0 0 0 0 -0.3" size="0.03" contype="0" conaffinity="0"/>
<body pos="0 0 -0.3">
<joint name="q2" type="hinge" axis="0 1 0" range="0 2.4"/>
<!--  shin visual only  -->
<geom name="left_shin_geom" type="cylinder" fromto="0 0 0 0 0 -0.3" size="0.028" contype="0" conaffinity="0"/>
<!--  FOOT (contact)  -->
<geom name="left_foot" type="box" size="0.07 0.035 0.02" pos="0.09 0 -0.30"/>
<site name="left_foot_site" pos="0.09 0 -0.30" size="0.01"/>
</body>
</body>
<!--  RIGHT LEG  -->
<body pos="0 0.1 -0.2">
<joint name="q3" type="hinge" axis="0 1 0" range="-1.2 0.6"/>
<!--  thigh visual only  -->
<geom name="right_thigh_geom" type="cylinder" fromto="0 0 0 0 0 -0.3" size="0.03" contype="0" conaffinity="0"/>
<body pos="0 0 -0.3">
<joint name="q4" type="hinge" axis="0 1 0" range="0 2.4"/>
<!--  shin visual only  -->
<geom name="right_shin_geom" type="cylinder" fromto="0 0 0 0 0 -0.3" size="0.028" contype="0" conaffinity="0"/>
<!--  FOOT (contact)  -->
<geom name="right_foot" type="box" size="0.07 0.035 0.02" pos="0.09 0 -0.30"/>
<site name="right_foot_site" pos="0.09 0 -0.30" size="0.01"/>
</body>
</body>
</body>
</worldbody>
<actuator>
<position joint="q1" kp="60" forcerange="-80 80"/>
<position joint="q3" kp="60" forcerange="-80 80"/>
<position joint="q2" kp="90" forcerange="-120 120"/>
<position joint="q4" kp="90" forcerange="-120 120"/>
</actuator>
<!--  KEYFRAME sets the true initial qpos   -->
<!--  qpos order: rootx rootz rootyaw theta q1 q2 q3 q4  -->
<keyframe>
<key name="start" qpos="0 1.42 0 0 0 1.05 0 1.05" qvel="0 0 0 0 0 0 0 0"/>
</keyframe>
</mujoco>
